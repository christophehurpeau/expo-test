{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmit": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "./dist-js"
  },
  "exclude": [
      "node_modules",
      "./dist-js"
  ]
}
